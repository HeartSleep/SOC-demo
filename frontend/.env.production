# Production Environment
VITE_MODE=production

# API Configuration
VITE_API_BASE_URL=https://api.soc-platform.com
VITE_API_TIMEOUT=30000

# Production Optimizations
VITE_ENABLE_MOCK=false
VITE_DROP_CONSOLE=true
VITE_DROP_DEBUGGER=true
VITE_SOURCEMAP=false

# Build Configuration
VITE_BUILD_COMPRESS=gzip
VITE_BUILD_COMPRESS_DELETE_ORIGIN_FILE=true

# CDN
VITE_USE_CDN=true
VITE_CDN_URL=https://cdn.jsdelivr.net/npm

# PWA
VITE_ENABLE_PWA=true

# Security
VITE_ENABLE_ENCRYPT_STORAGE=true

# Error Logging
VITE_ENABLE_ERROR_LOG=true
VITE_ERROR_LOG_LEVEL=error